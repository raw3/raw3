{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "extends": [
    "rxjs-tslint-rules",
    "tslint-angular",
    "tslint-eslint-rules"
  ],
  "rules": {
    "array-bracket-spacing": [
      true,
      "never"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "arrow-return-shorthand": true,
    "callable-types": true,
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "no-unnecessary-type-assertion": [
      true,
      "FormControl"
    ],
    "jsdoc-format": false,
    "curly": true,
    "deprecation": {
      "severity": "warn"
    },
    "eofline": true,
    "forin": true,
    "import-blacklist": [
      true,
      "rxjs/Rx"
    ],
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "new-parens": true,
    "interface-over-type-literal": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-duplicate-variable": [
      true,
      "check-parameters"
    ],
    "label-position": true,
    "max-line-length": [
      false,
      140
    ],
    "space-within-parens": false,
    "member-ordering": [
      true,
      {
        "order": [
          "private-static-field",
          "private-instance-field",
          "public-static-field",
          "public-instance-field",
          "private-constructor",
          "public-constructor",
          "private-instance-method",
          "protected-instance-method",
          "public-instance-method"
        ]
      }
    ],
    "no-arg": true,
    "no-bitwise": true,
    "ban": [
      true,
      [
        "alert"
      ]
    ],
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace",
      "log"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-empty": false,
    "no-empty-interface": true,
    "no-eval": true,
    "no-inferrable-types": [
      true,
      "ignore-params"
    ],
    "space-before-function-paren": [
      true,
      "always"
    ],
    "no-misused-new": true,
    "no-non-null-assertion": true,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unnecessary-initializer": true,
    "no-unused-expression": true,
    "no-duplicate-imports": true,
    "no-use-before-declare": true,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-var-keyword": true,
    "no-irregular-whitespace": true,
    "no-angle-bracket-type-assertion": true,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "prefer-const": true,
    "quotemark": [
      true,
      "single"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "unified-signatures": true,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "directive-selector": [
      true,
      "attribute",
      [
        "app",
        "raw3"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "app",
        "raw3"
      ],
      "kebab-case"
    ],
    "no-output-on-prefix": true,
    "no-input-rename": true,
    "no-output-rename": true,
    "use-life-cycle-interface": true,
    "use-pipe-transform-interface": true,
    "component-class-suffix": true,
    "directive-class-suffix": true,
    "import-destructuring-spacing": false,
    "array-type": [
      true,
      "array"
    ],
    "ban-comma-operator": true,
    "max-classes-per-file": false,
    "use-view-encapsulation": false,
    "no-param-reassign": [
      "error",
      {
        "props": true
      }
    ],
    "rxjs-no-unsafe-takeuntil": true,
    "rxjs-finnish": {
      "options": [
        {
          "functions": true,
          "methods": true,
          "parameters": true,
          "properties": true,
          "variables": true
        }
      ],
      "severity": "error"
    },
    "rxjs-no-internal": true,
    "rxjs-ban-operators": true,
    "rxjs-no-subject-value": true,
    "rxjs-no-unsafe-switchmap": true
  }
}
