{
  "name": "raw3",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raw3/raw3"
  },
  "author": {
    "email": "info@raw3.io",
    "name": "Richard Wols",
    "url": "https://raw3.io"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "watch": "ng build --prod --watch",
    "lint": "ng lint raw3",
    "build:client-and-server-bundles": "ng build --prod && ng run raw3:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "watch:ssr": "ng run raw3:server:production && npm run webpack:watch",
    "serve:ssr": "node dist/server/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "webpack:watch": "webpack --config webpack.server.config.js --progress --colors --watch",
    "serve:prod": "cd ../production && node dist/server/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.3",
    "@angular/common": "8.1.3",
    "@angular/compiler": "8.1.3",
    "@angular/core": "8.1.3",
    "@angular/forms": "8.1.3",
    "@angular/platform-browser": "8.1.3",
    "@angular/platform-browser-dynamic": "8.1.3",
    "@angular/platform-server": "8.1.3",
    "@angular/pwa": "0.801.2",
    "@angular/router": "8.1.3",
    "@angular/service-worker": "8.1.3",
    "@datorama/akita": "^4.8.1",
    "@datorama/akita-ngdevtools": "3.0.2",
    "@google/markerclusterer": "1.0.3",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "core-js": "2.5.7",
    "rxjs": "6.5.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.2",
    "@angular/cli": "8.1.2",
    "@angular/compiler-cli": "8.1.3",
    "@angular/language-service": "8.1.3",
    "@types/express": "4.17.0",
